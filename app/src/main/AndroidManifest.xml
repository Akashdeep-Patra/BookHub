    package="com.example.lucifer.firebase">
<uses-permission android:name="android.permission.CALL_PHONE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".inputForms.login"
            android:noHistory="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".HomePageActivity"
            android:theme="@style/AppTheme.home">

        </activity>
        <activity android:name=".inputForms.SignupActivity" />
        <activity android:name=".ProfileActivity" />
        <activity
            android:name=".inputForms.FormUser"
            android:label="@string/title_activity_form_user" />
        <activity android:name=".bookList" />
        <activity android:name=".inputForms.bookForm" />
        <activity android:name=".BookFull"></activity>
    </application>

</manifest>